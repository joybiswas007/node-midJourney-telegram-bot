{
  "type": "module",
  "dependencies": {
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "midjourney": "^4.3.11",
    "mongoose": "^7.5.0",
    "node-telegram-bot-api": "^0.61.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5"
  },
  "name": "node-midjourney-telegram-bot",
  "version": "1.1.2",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "server:up": "pm2 start app.js --name node-mj --watch",
    "server:restart": "pm2 restart app.js",
    "server:down": "pm2 delete app.js"
  },
  "keywords": [
    "midjourney",
    "node",
    "telegram",
    "bot"
  ],
  "author": "joybiswas007",
  "license": "MIT",
  "description": "A Telegram bot created in Node.js using the unofficial Midjourney Node.js client."
}
